{
  "name": "nr-ngo-validation-serverless",
  "version": "1.1.1",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "NODE_ENV=test jest --passWithNoTests --coverage",
    "eslint-check": "eslint src/**/*.ts",
    "check": "ncu -e 2",
    "upgrade": "ncu -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newrelic/nr-ngo-validation-serverless.git"
  },
  "bugs": {
    "email": "opensource+nr-ngo-validation-serverless@newrelic.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/aws-lambda": "^8.10.81",
    "@types/data-api-client": "^1.2.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.6",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.6",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "npm-check-updates": "^11.8.3",
    "prettier": "^2.3.2",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-newrelic-lambda-layers": "1.1.8",
    "serverless-offline": "^8.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "aws-sdk": "2.956.0",
    "data-api-client": "1.2.0",
    "envalid": "^7.1.1",
    "http-status-codes": "^2.1.4",
    "myzod": "1.8.4",
    "node-fetch": "^2.6.1",
    "npm-check": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "modulePathIgnorePatterns": [
      "src/types/",
      "src/functions/",
      "src/utils/org-validator.ts",
      "src/utils/status.ts",
      "src/utils/logger.ts"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "feat",
          "fix",
          "refactor",
          "revert",
          "test",
          "deps"
        ]
      ],
      "scope-empty": [
        2,
        "always"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-min-length": [
        2,
        "always",
        10
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "subject-case": [
        2,
        "always",
        "lower-case"
      ]
    }
  }
}
